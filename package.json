{
  "name": "@flycode-org/eslint-plugin-react-i18next",
  "description": "ESLint rules for react-i18next",
  "version": "1.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "eslint .",
    "format": "prettier --write .",
